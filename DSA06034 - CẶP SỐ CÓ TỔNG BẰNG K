#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ;

using namespace std;

// bai toan quy ve tim so luong lon hon bang  

int main ()
{
	int t ; cin >> t;
	while(t--) {
		int n , k; cin >> n >> k ; 
		int a[n];
		for(int i = 0 ; i < n ; i++) cin >> a[i];
		sort(a,a+n);
		ll ans = 0 ; 
	 	for(int i = 0 ; i < n - 1; i++) {
	 		auto *it1 = upper_bound(a + i + 1 , a + n , k - a[i]) ;
	 		auto *it2 = lower_bound(a + i + 1 , a + n , k - a[i]) ;
	 		ans += it1 - it2; 
		} 
		 cout << ans << endl  ;
	} 
}
