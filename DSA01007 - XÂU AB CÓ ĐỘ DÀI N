#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ; 
int n;
int x[20]; 

void result() {
	for(int i = 1 ; i <= n ; i++)
	{
		if(x[i] == 0) cout << "A";
		else cout << "B"; 
	 } 
	 cout << " "; 
} 

void Try(int i) 
{
	for(int j = 0; j <= 1 ; j++) {
		x[i] = j ;
		if(i == n) result();
		else {
			Try(i + 1); 
		} 
	} 
} 

int main () 
{
	int t ; cin >> t ;
	while(t--)
	{
		cin >> n ; 
		Try(1);  
		e; 
	} 
} 















