#include <bits/stdc++.h>

using namespace std;

#define ll long long
#define e cout << endl
#define pb push_back 

using namespace std;

int main ()
{
	int n ; cin >> n ; 
	int a[n] ;
	for(int i = 0 ; i < n ; i++) cin >> a[i];
	
	vector<vector<int> > ans ; 
	for(int i = 0 ; i < n - 1 ; i++)
	{	
		vector<int> v ; 
		int min = i ;
		for(int j = i + 1 ; j < n ; j++)
		{
			if(a[j] < a[min]) {
				min = j ; 
			} 
		 } 
		 swap(a[i],a[min]); 
		 
		for(int x : a) {
			v.pb(x); 
		}
		ans.pb(v); 
	} 
	for(int i = ans.size() - 1 ; i >= 0 ; i--) 
	{	
		cout << "Buoc " << i + 1<< ": "; 
		for(int j = 0 ; j < n ; j++) {
			cout << ans[i][j] << " " ;
		} 
		e; 
	}
}
