#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ;  

int main ()
{
	int n ; cin >> n ; int a[n];
	for(int i = 0 ; i < n ; i++) cin >> a[i];
	sort(a,a+n); 
	do {
		for(int i = 0 ; i < n ; i++) {
			cout << a[i] << " "; 
		} 
		e; 
	} while(next_permutation(a,a+n)); 
}
