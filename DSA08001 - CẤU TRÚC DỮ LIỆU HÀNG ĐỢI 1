#include <bits/stdc++.h>

using namespace std;

#define ll long long
#define e cout << endl

using namespace std;

int main ()
{
	int t ; cin >> t;
	while(t--) {
		queue<int> q;
		int n ; cin >> n ; // n la so thao tac
		while(n--) {
			int tt ; cin >> tt ;
			if(tt == 1) {
				cout << q.size() << endl ;
			}
			else if(tt == 2) {
				if(q.empty()) {
					cout << "YES\n";
				}
				else cout << "NO\n";
			}
			else if(tt == 3) {
				int x ; cin >> x ;
				q.push(x);
			}
			else if(tt == 4) {
				if(!q.empty()) {
					q.pop(); // ham pop xoa phan tu o dau hang
				}
			}
			else if(tt == 5) {
				if(!q.empty()) {
					cout << q.front() << endl ; // tra ve ptu dau
				} else cout << -1 <<endl ;
			}
			else if(tt == 6) {
				if(!q.empty()) {
					cout << q.back() << endl ; // tra ve ptu cuoi
				} else cout << -1 <<endl ;			
			}
		}		
	}
}
