#include <bits/stdc++.h>

#define ll long long 
#define pb push_back

using namespace std ;

int main ()
{
	int t ; cin >> t ;
	while(t--) {
		int cnt= 0 ; 
		int n , x ; cin >> n >> x;
		int a[n];
		for(int i = 0 ; i < n ; i++) {
			cin >> a[i]; 
			if (a[i]==x) {
				++cnt ; 
			}
			
		}
		if(cnt != 0) {
			cout << cnt << endl ; 
		} 
		else {
			cout << "-1" << endl ; 
		} 
	} 
	 
}
