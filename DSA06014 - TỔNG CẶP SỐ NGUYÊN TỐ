#include <bits/stdc++.h> 

#define ll long long

using namespace std;

int prime[1000001];

void sang()

{
	
	for(int i = 2 ; i <= 1000000 ; i++) {
		prime[i] = 1 ;
	}
	
	for(int i = 2 ; i <= sqrt(1000000) ; i++)
	{
		if(prime[i])
		{
			for(int j = i * i ; j <= 1000000 ; j += i)
			{
				prime[j] = 0 ;
			}
		}
	}
}
int main ()
{
	int t ; cin >> t ;
	sang();
	while(t--)
	{
		int check = 1 ;
		int n ; cin >> n ;
		for(int i = 2 ; i <= n / 2; i++) {
			if(prime[i] && prime[n - i])
			{
				check = 0 ;
				cout << i << " " << n - i << endl;
				break;
			}
		}
		if(check) cout << "-1\n" ;
	}
}
