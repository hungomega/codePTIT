int main () {
	int t ; cin >> t ;
	while(t--) {
		int n ; cin >> n ; int a[n] ;
		for(int &x : a) cin >> x ;
		next_permutation(a,a+n) ;
		for(int i = 0 ; i < n ; i++) {
			cout << a[i] << " "; 
		} 
