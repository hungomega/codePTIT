#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl 

int n , x[100];
bool check = false ;

int tn(int a[]) {
	for(int i = 1 ; i <= n / 2 ; i++) {
		if(a[i] != a[n - i + 1]) return 0 ; 
	} 
	return 1 ; 
} 

void first() {
	for(int i = 1 ; i <= n ; i++) x[i] = 0 ; 
}

void sinh() {
	int i = n ;
	while(i >= 1 && x[i] == 1) {
		x[i] = 0;
		i--; 
	}
	if(i == 0) check = true ;
	else {
		x[i] = 1 ; 
	} 
} 

int main ()
{
	cin >> n ;
	while(!check) {
		if(tn(x)) {
			for(int i = 1 ; i <= n ; i++) cout << x[i] << " ";
			e; 
		} 
		sinh() ; 
	} 
 } 














