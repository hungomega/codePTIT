#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>
#define vll vector<long long> 
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl 

int main() {
	int t ; cin >> t;
	while(t--) {
		int n; cin >> n;
		int a[100000];
		vector<vector<int> > ans ; 
		for (int i = 0; i < n; i++) cin >> a[i];
		for (int i = 0; i < n - 1; i++) {
			vector<int> v ; 
			for (int j = i + 1; j < n; j++) {
				if (a[i] > a[j]) {
					swap(a[i], a[j]);
				}
			}
			for(int x : a) {
				v.pb(x); 
			}
			ans.pb(v); 
		}
		for(int i = ans.size() - 1 ; i >= 0 ; i--) 
		{	
			cout << "Buoc " << i + 1 << ": "; 
			for(int j = 0 ; j < n ; j++) {
				cout << ans[i][j] << " " ;
			} 
			e; 
		}		
	} 
}
