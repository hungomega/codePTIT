#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ;  

int main () {
	int t ; cin >> t ;
	while(t--)
	{
		int n ; cin >> n ;
		vector<string> v; 
		v.pb("0");
		v.pb("1");
		for(int i = 2 ; i <= n ; i++) {
			for(int j = v.size() - 1 ; j >= 0 ; j--) {
				v.pb("1"+v[j]); // 10
				v[j] = "0" + v[j] ;
			} 
		} 
		for(string &s : v) cout << s << " " ;
		e; 
	 } 
}
