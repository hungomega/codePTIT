#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ; 

int b[20] , n; 
void in() {
	for(int i = 1 ; i <= n ; i++) 
	{
		if(b[i]) cout << 'H' ;
		else cout << 'A' ; 
	} 
	e; 
} 
 
void Try(int i)
{
	if(i == 1) 
	{
		b[i] = 1 ;
		Try(i+1);
		return ; 
	} 
	if(i == n)
	{
		// ky tu cuoi la a  
		b[i] = 0 ;
		in();
		return;  
	} 
	if(i > 1) {
		for(int j = 0 ; j <= 1 ; j++) {	  
			if(j == 1 && b[i - 1] == 1) { 
				continue ; 
			} 
			b[i] = j ;
			Try(i + 1) ;
		} 	
	} 
} 

int main ()
{
	int t ; cin >> t;
	while(t--) {
		cin >> n ; 
		Try(1);	  
	} 
	return 0 ; 
}
