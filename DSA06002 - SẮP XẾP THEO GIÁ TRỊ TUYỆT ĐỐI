#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ; 

using namespace std ; 

int n , x ; 

int cmp(int a ,int b) {
	 return abs(a - x) < abs(b - x) ;
} 

int main ()
{
	int t ; cin >> t ;
	while(t--)
	{
		cin >> n >> x ;
		int a[n];
		for(int &x : a) cin >> x ; 
		stable_sort(a,a+n,cmp);
		for(int x : a) cout << x << " ";
		e; 
	} 
}
