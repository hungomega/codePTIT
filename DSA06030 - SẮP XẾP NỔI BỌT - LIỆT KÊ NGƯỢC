#include <bits/stdc++.h>

#define ll long long 
#define pb push_back
#define e cout << endl  
using namespace std ; 

int main () {
	int t ; cin >> t ;
	while(t--)
	{
		int n ; cin >> n ;
		int a[n]; 
		for(int i = 0 ; i < n ; i++) cin >> a[i]; 
		
		vector<vector<int> > ans ; 	
		for(int i = 0 ; i <= n - 2 ; i++) {
			int swapped = 0 ; 
			vector<int> v; 
			for(int j = 0 ; j <= n - 2 - i ; j++) {
				if(a[j] > a[j + 1]) {
					swap(a[j],a[j + 1]); 
					swapped = 1 ; 
				} 	
			} 
			if(swapped == 0) {
				break ;
			}
			for(int x : a) {
				v.pb(x); 
			}
			ans.pb(v); 
		} 
		for(int i = ans.size() - 1 ; i >= 0 ; i--) 
		{	
			cout << "Buoc " << i + 1<< ": "; 
			for(int j = 0 ; j < n ; j++) {
				cout << ans[i][j] << " " ;
			} 
			e; 
		}	
	} 
}
