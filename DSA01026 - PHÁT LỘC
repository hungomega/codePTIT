#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ; 

int b[25] , n; 
void in() {
	for(int i = 1 ; i <= n ; i++) 
	{
		if(b[i]) cout << 8 ;
		else cout << 6 ; 
	} 
	e; 
} 
 
void Try(int i)
{
	if(i == 1) 
	{
		b[i] = 1 ;
		Try(i+1);
		return ; 
	} 
	if(i == n)
	{
		// khong co nhieu hon 3 chu so 6 dung canh nhau 
		if(b[i - 1] == 0 && b[i - 2] == 0 && b[i - 3] == 0) {
			return ; 
		} 
		b[i] = 0 ;
		in();
		return ; 
	} 
	for(int j = 0 ; j <= 1 ; j++) {
		// co 2 chu so 8 dung canh nhau 
		if(j == 1 && b[i - 1] == 1) continue ; 
		// ko co 4 so 6 lien nhau 
		if(j == 0 && b[i - 1] == 0 && b[i - 2] == 0&& b[i - 3] == 0) {
			continue ; 
		} 
		b[i] = j ;
		Try(i + 1) ;
	} 
} 

int main ()
{
	cin >> n ; 
	Try(1);
	return 0 ; 
}
