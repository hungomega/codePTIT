#include <bits/stdc++.h>

#define ll long long 
#define pb push_back

using namespace std ; 
// Æ° tuong : o moi buoc se dua phan tu chua dua sap xep len dau day 
int main () {
	int n ; cin >> n ;
	int a[n]; 
	for(int i = 0 ; i < n ; i++) cin >> a[i]; 
	for(int i = 0 ; i <= n - 2 ; i++) {
		int swapped = 0 ; 
		for(int j = 0 ; j <= n - 2 - i ; j++) {
			if(a[j] > a[j + 1]) {
				swap(a[j],a[j + 1]); 
				swapped = 1 ; 
			} 	
		} 
		if(swapped == 0) {
			break ;
		}
		cout << "Buoc " << i + 1 << ": ";  
		for(int i = 0 ; i < n ; i++) {
			cout << a[i] << " "; 
		} 
		cout << endl ; 
	} 
}
