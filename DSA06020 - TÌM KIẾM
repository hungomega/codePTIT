#include <bits/stdc++.h>

#define ll long long 
#define pb push_back

using namespace std ;

int main ()
{
	int t ; cin >> t ; 
	while(t--)
	{
		int n , x ; cin >> n >> x ; 
		int a[n] ; 
		bool ok = true ; 
		for(int i = 0 ; i < n ; i++) {
			cin >> a[i];  
		} 
		for(int i = 0 ; i < n ; i++) {
			if(a[i] == x) {
				cout << "1\n";
				ok = false ; 
				break; 
			} 
		} 
		if(ok) cout << "-1\n"; 
	}
}
