#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ; 

void sinh_Preversed(string &s) {
	int i = s.size() - 1 ;
	while(i >= 0 && s[i] == '0') {
		s[i] = '1' ;
		--i ;
	}
	if(i >= 0) {
		s[i] = '0' ;
	} else {
		for(char &c : s) c = '1';
	}
}

int main () 
{
	int t ; cin >> t ;
	while(t--)
	{
		string s ; cin >> s ;
		sinh_Preversed(s);
		cout << s << endl ; 
	} 
	return 0 ; 
}
