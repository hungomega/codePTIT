#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl 

int n , k , x[100];
bool check = false; 

void first() {
	for(int i = 1 ; i <= k ; i++) x[i] = i ; 
} 

void sinh() {
	int i = k ;
	while(x[i] == n - k + i && i >= 1) {
		--i ; 
	}
	if(i == 0) check = true ;
	else {
		++x[i];
		for(int j = i + 1 ; j <= k ; j++) 
		{
			x[j] = x[j - 1] + 1 ; 
		 } 
	} 
} 

int main ()
{
	int t ; cin >> t ;
	while(t--) {
		cin >> n >> k ;
		first(); 
		while(!check) {
			for(int i = 1 ; i <= k ; i++) cout << x[i] ;
			cout << " "; 
			sinh(); 
		} 
		e; 
		check = false ; 
	} 
}
