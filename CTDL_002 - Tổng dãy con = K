#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ; 

int n , k ; int a[1005] , b[1005] , cnt = 0;
 
void result(int m) {
	for(int i = 1 ; i <= m ; i++) 
	{
		if(b[i]) cout << a[i] << "       "; 
	 } 
	e;
	++cnt ; 
} 

void Try(int i , int sum) {
	for(int j = 0 ; j <= 1 ; j++) 
	{
		b[i] = j ; 
		if(sum + a[i]*j == k) result(i); 
		else if(i < n) Try(i + 1 , sum + a[i]*j);
	 } 
} 

int main () {
	cin >> n >> k ;
	for(int i = 1 ; i <= n ; i++) cin >> a[i]; 
	Try(1,0);
	cout << cnt << endl ;
	return 0; 
}



















