#include <bits/stdc++.h> 

#define ll long long
#define pb push_back  

using namespace std;

 int c[2000000];
 
int main ()
{
	int t ; cin >> t ;
	while(t--) {
		int n1 , n2 ; cin >> n1 >> n2 ;
		int a[n1] , b[n2];
		for(int i = 0 ; i < n1 ; i++) cin >> a[i];
		for(int i = 0 ; i < n2 ; i++) cin >> b[i];
		sort(a,a+n1);
		sort(b,b+n2);
		int index = 0 ; 
		
		int i = 0 , j = 0 ;
		while(i < n1 && j < n2) 
		{
			if(a[i] <= b[j]) 
			{
				c[index++] = a[i++] ; 
			} else {
				c[index++] = b[j++] ; 
			} 
		}
			while(i < n1) {
				c[index++] = a[i++] ; 
			} 
			while(j < n2) {
				c[index++] = b[j++]; 
			} 
		
		for(int i = 0 ; i < index ; i++) {
			cout << c[i] << " "; 
		} 
		cout << endl ; 
	} 
} 














 
