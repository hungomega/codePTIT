#include <bits/stdc++.h>

using namespace std;

#define ll long long
#define e cout << endl
#define pb push_back 

using namespace std;

int main ()
{
	int t; cin >> t;
	while(t--) {
		int n ; cin >> n ;
		queue<ll> q ; 
		q.push(9) ;
		while(1) {
			 ll x = q.front();
			 if(x % n == 0) {
			 	cout << x << endl ;
				 break ; 
			 } 
			 q.pop();
			 q.push(x*10);
			 q.push(x*10 + 9); 
		} 
	} 

}
