#include <bits/stdc++.h>

using namespace std;

#define pb push_back 
#define ll long long 
#define vi vector<int>	
#define vc vector<char>	
#define vll vector<long long>
#define vpi vector<pair<int,int>>  
#define FOR(i,a,b) for(int i = a ; i < b ; i++)  
#define e cout << endl ; 

using namespace std ; 

ll n , k ; 

ll dem(ll a[] , ll l , ll r , ll value) {
	ll *it1 = lower_bound(a+l,a+r,value) ;
	ll *it2 = upper_bound(a+l,a+r,value) ;
	return it2 - it1; 
} 

int main () {
	int t ; cin >> t;
	while(t--) {
		cin >> n >> k ; ll a[n]; 
		for(ll i = 0 ; i < n ; i++) cin >> a[i];
		sort(a,a+n); 
		ll cnt = 0 ;
		
		for(int i = 0 ; i < n - 1 ; i++) {
			cnt += dem(a,i+1,n,k-a[i]); 
		} 
		cout << cnt << endl ; 
	} 
}




















